<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <union-subclass name="uk.ac.ebi.jmzidml.model.mzidml.SpectraData" table="SpectraData"
                    extends="uk.ac.ebi.jmzidml.model.mzidml.ExternalData">

        <many-to-one name="spectrumIDFormat"
                     class="uk.ac.ebi.jmzidml.model.mzidml.SpectrumIDFormat" column="spectrumIDFormat_hid"
                     not-null="true"/>
        
    </union-subclass>
</hibernate-mapping>
