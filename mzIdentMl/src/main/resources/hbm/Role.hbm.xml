<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="uk.ac.ebi.jmzidml.model.mzidml.Role" table="Role">
        <id name="hid" type="java.lang.Long" access="field">
            <column name="hid" />
            <generator class="org.hibernate.id.enhanced.SequenceStyleGenerator" />
        </id>
        <many-to-one name="cvParam" cascade="all" class="uk.ac.ebi.jmzidml.model.mzidml.CvParam"/>
    </class>
</hibernate-mapping>
