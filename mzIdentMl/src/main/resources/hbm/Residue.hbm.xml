<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="uk.ac.ebi.jmzidml.model.mzidml.Residue" table="Residue">
        <id name="hid" access="field" type="java.lang.Long">
            <column name="hid"/>
            <generator class="org.hibernate.id.enhanced.SequenceStyleGenerator"/>
        </id>
        <property name="code" type="java.lang.String">
            <column name="code"/>
        </property>
        <property name="mass" type="java.lang.Float">
            <column name="mass" precision="12" scale="0" not-null="true"/>
        </property>

        <!--<property name="softwareRef" type="java.lang.String">-->
