<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="uk.ac.ebi.jmzidml.model.mzidml.DatabaseFilters" table="DatabaseFilters" catalog="mzidentml">
        <id name="hid" type="java.lang.Long">
            <column name="hid" />
            <generator class="sequence" />
        </id>
        <list name="filter" cascade="all">
            <key column="hid"/>
            <index column="idx"/>
            <one-to-many class="uk.ac.ebi.jmzidml.model.mzidml.Filter"/>
    </list>
    </class>
</hibernate-mapping>
