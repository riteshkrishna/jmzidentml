<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <union-subclass name="uk.ac.ebi.jmzidml.model.mzidml.MassTable" table="MassTable" catalog="mzidentml"
                    extends="uk.ac.ebi.jmzidml.model.AbstractIdentifiableParamGroup">
        <list name="paramGroup" cascade="all">
            <key column="paramGroup_hid"/>
            <index column="paramGroup_idx"/>
            <one-to-many class="uk.ac.ebi.jmzidml.model.mzidml.Param"/>
        </list>
        <list name="residue" cascade="all">
            <key column="residue_hid"/>
            <index column="residue_idx"/>
            <one-to-many class="uk.ac.ebi.jmzidml.model.mzidml.Residue"/>
        </list>
        <list name="ambiguousResidue" cascade="all">
            <key column="ambiguousResidue_hid"/>
            <index column="ambiguousResidue_idx"/>
            <one-to-many class="uk.ac.ebi.jmzidml.model.mzidml.AmbiguousResidue"/>
        </list>
        <list name="msLevel">
            <key column="hid"/>
            <list-index column="msLevel_index"/>
            <element type="java.math.BigInteger"/>
        </list>
    </union-subclass>
</hibernate-mapping>