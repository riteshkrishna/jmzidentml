<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="uk.ac.ebi.jmzidml.model.mzidml.AnalysisCollection" table="AnalysisCollection" >
        <id name="hid" access="field" type="java.lang.Long">
            <generator class="org.hibernate.id.enhanced.SequenceStyleGenerator" />
        </id>

    <list name="spectrumIdentification" cascade="all" table="AnalysisCollection_SpectrumIdentification">
            <key column="AnalysisCollection_hid"/>
            <index column="idx"/>
            <many-to-many column="SpectrumIdentification_hid" class="uk.ac.ebi.jmzidml.model.mzidml.SpectrumIdentification"/>
    </list>
    <many-to-one name="proteinDetection" cascade="all" class="uk.ac.ebi.jmzidml.model.mzidml.ProteinDetection"/>

    </class>
</hibernate-mapping>
