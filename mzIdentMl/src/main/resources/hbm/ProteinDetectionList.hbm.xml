<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<union-subclass name="uk.ac.ebi.jmzidml.model.mzidml.ProteinDetectionList"
		table="ProteinDetectionList" extends="uk.ac.ebi.jmzidml.model.mzidml.InternalData">

        <list name="paramGroup" cascade="all" access="field" >
            <key column="ProteinDetectionList_hid"/>
            <index column="idx"/>
            <one-to-many class="uk.ac.ebi.jmzidml.model.mzidml.Param"/>
        </list>

        <list name="proteinAmbiguityGroup" access="field">
            <key column="ProteinAmbiguityGroup_hid"/>
            <index column="idx"/>
            <one-to-many class="uk.ac.ebi.jmzidml.model.mzidml.ProteinAmbiguityGroup"/>
        </list>

	</union-subclass>
</hibernate-mapping>
