<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="uk.ac.ebi.jmzidml.model.mzidml.AnalysisData" table="AnalysisData" >
        <id name="hid" type="java.lang.Long" access="field">
            <generator class="org.hibernate.id.enhanced.SequenceStyleGenerator" />
        </id>

        <list name="spectrumIdentificationList" cascade="all">
            <key column="hid"/>
            <index column="idx"/>
            <one-to-many class="uk.ac.ebi.jmzidml.model.mzidml.SpectrumIdentification"/>
        </list>
        <many-to-one name="proteinDetectionList" cascade="all" class="uk.ac.ebi.jmzidml.model.mzidml.ProteinDetection"/>

        <many-to-one name="proteinDetectionList" cascade="all" class="uk.ac.ebi.jmzidml.model.mzidml.ProteinDetection"/> 
        <!--<property name="proteinDetectionList" type="uk.ac.ebi.jmzidml.model.mzidml.ProteinDetectionList"/>-->
    </class>
</hibernate-mapping>
