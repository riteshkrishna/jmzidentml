        <!--</property>-->
        <!--<many-to-one name="contactRole" class="uk.ac.ebi.jmzidml.model.mzidml.ContactRole" column="contactRole" cascade="all"/>-->
        <!--<many-to-one name="analysisSoftware" class="uk.ac.ebi.jmzidml.model.mzidml.AnalysisSoftware" column="analysisSoftware_hid" cascade="all"/>-->
        
    </class>
</hibernate-mapping>
