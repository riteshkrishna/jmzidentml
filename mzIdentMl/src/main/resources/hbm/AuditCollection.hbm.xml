<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="uk.ac.ebi.jmzidml.model.mzidml.AuditCollection"  table="AuditCollection" >
        <id name="hid" type="java.lang.Long">
            <generator class="org.hibernate.id.enhanced.SequenceStyleGenerator" />
        </id>

    <list name="contactGroup" table="AuditCollection_Contact" cascade="all" access="field">
            <key column="AuditCollection_hid"/>
            <index column="idx"/>
            <many-to-many column="Contact_hid" class="uk.ac.ebi.jmzidml.model.mzidml.Contact"/>
    </list>
    </class>
</hibernate-mapping>
