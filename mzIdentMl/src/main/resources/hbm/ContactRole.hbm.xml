<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="uk.ac.ebi.jmzidml.model.mzidml.ContactRole" table="ContactRole" catalog="mzidentml">
        <id name="hid" type="java.lang.Long">
            <column name="hid" />
            <generator class="org.hibernate.id.enhanced.SequenceStyleGenerator" />
        </id>
        <many-to-one name="role" cascade="all" class="uk.ac.ebi.jmzidml.model.mzidml.Role"/>
        <many-to-one name="contact" cascade="all" class="uk.ac.ebi.jmzidml.model.mzidml.Contact"/>
        <property name="contactRef" type="java.lang.String"/>
    </class>
</hibernate-mapping>
