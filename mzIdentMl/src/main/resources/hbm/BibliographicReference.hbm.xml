<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <union-subclass name="uk.ac.ebi.jmzidml.model.mzidml.BibliographicReference" table="BibliographicReference" catalog="mzidentml" extends="uk.ac.ebi.jmzidml.model.mzidml.Identifiable">
        <property name="authors" type="java.lang.String"/>
        <property name="publication" type="java.lang.String"/>
        <property name="publisher" type="java.lang.String"/>
        <property name="editor" type="java.lang.String"/>
        <property name="year" type="java.lang.Integer"/>
        <property name="volume" type="java.lang.String"/>
        <property name="issue" type="java.lang.String"/>
        <property name="pages" type="java.lang.String"/>
        <property name="title" type="java.lang.String"/>

    </union-subclass>

</hibernate-mapping>
