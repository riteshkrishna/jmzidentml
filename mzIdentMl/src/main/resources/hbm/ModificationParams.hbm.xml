<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="uk.ac.ebi.jmzidml.model.mzidml.ModificationParams" table="ModificationParams" catalog="mzidentml">
        <id name="hid" type="java.lang.Long">
            <column name="hid"/>
            <generator class="sequence"/>
        </id>
        <list name="searchModification" cascade="all">
            <key column="searchModification_hid"/>
            <index column="searchModification_idx"/>
            <one-to-many class="uk.ac.ebi.jmzidml.model.mzidml.SearchModification"/>
        </list>
    </class>
</hibernate-mapping>