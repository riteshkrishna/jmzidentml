<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="uk.ac.ebi.jmzidml.model.mzidml.AnalysisCollection" table="AnalysisCollection" catalog="mzidentml">
        <id name="hid" type="java.lang.Long">
            <generator class="sequence" />
        </id>

    <list name="spectrumIdentification" cascade="all">
            <key column="hid"/>
            <index column="idx"/>
            <one-to-many class="uk.ac.ebi.jmzidml.model.mzidml.SpectrumIdentification"/>
    </list>
    <!-- <many-to-one name="proteinDetection" cascade="all" class="uk.ac.ebi.jmzidml.model.mzidml.ProteinDetection"/> -->
        <property name="proteinDetection" type="uk.ac.ebi.jmzidml.model.mzidml.ProteinDetection"/>
    </class>
</hibernate-mapping>
