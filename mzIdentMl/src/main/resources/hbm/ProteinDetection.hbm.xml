<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<union-subclass name="uk.ac.ebi.jmzidml.model.mzidml.ProteinDetection"
		table="ProteinDetection" extends="uk.ac.ebi.jmzidml.model.mzidml.ProtocolApplication">

		<!--<many-to-one name="proteinAmbiguityGroup" cascade="all"-->
			<!--column="proteinAmbiguityGroup_hid" not-null="true"-->
			<!--class="uk.ac.ebi.jmzidml.model.mzidml.ProteinAmbiguityGroup" />-->

		<many-to-one name="proteinDetectionProtocol" cascade="all"
			column="ProteinDetection_hid" not-null="true"
			class="uk.ac.ebi.jmzidml.model.mzidml.ProteinDetectionProtocol" />

		<many-to-one name="proteinDetectionList" cascade="all"
			column="ProteinDetectionList_hid" not-null="true"
			class="uk.ac.ebi.jmzidml.model.mzidml.ProteinDetectionList" />

		<list name="inputSpectrumIdentifications" inverse="true" cascade="all" access="field">
			<key column="ProteinDetection_hid" not-null="true" />
			<index column="InputSpecId_idx" />
			<one-to-many class="uk.ac.ebi.jmzidml.model.mzidml.InputSpectrumIdentifications" />
		</list>

	</union-subclass>
</hibernate-mapping>