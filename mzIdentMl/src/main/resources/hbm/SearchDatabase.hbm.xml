<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="uk.ac.ebi.jmzidml.model.mzidml.SearchDatabase" table="SearchDatabase" catalog="mzidentml">
        <id name="hid" type="java.lang.Long">
            <column name="hid"/>
            <generator class="identity"/>
        </id>
        <many-to-one name="analysisSearchDatabase" column="analysisSoftware_hid"
                     class="uk.ac.ebi.jmzidml.model.mzidml.AnalysisSoftware"/>
    </class>
</hibernate-mapping>
