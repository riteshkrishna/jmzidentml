<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="uk.ac.ebi.jmzidml.model.mzidml.Inputs" table="Inputs" >
        <id name="hid" access="field" type="java.lang.Long">
            <column name="hid"/>
            <generator class="sequence"/>
        </id>
        <list name="sourceFile" cascade="all">
            <key column="sourceFile_hid"/>
            <index column="sourceFile_idx"/>
            <one-to-many class="uk.ac.ebi.jmzidml.model.mzidml.SourceFile"/>
        </list>
        <list name="searchDatabase" cascade="all">
            <key column="searchDatabase_hid"/>
            <index column="searchDatabase_idx"/>
            <one-to-many class="uk.ac.ebi.jmzidml.model.mzidml.AnalysisSearchDatabase"/>
        </list>
        <list name="spectraData" cascade="all">
            <key column="spectraData_hid"/>
            <index column="spectraData_idx"/>
            <one-to-many class="uk.ac.ebi.jmzidml.model.mzidml.SpectraData"/>
        </list>
    </class>
</hibernate-mapping>