<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <union-subclass name="uk.ac.ebi.jmzidml.model.mzidml.AnalysisSearchDatabase" table="AnalysisSearchDatabase" catalog="mzidentml" extends="uk.ac.ebi.jmzidml.model.mzidml.Identifiable">
        <property name="version" type="java.lang.String"/>
        <property name="releaseDate" type="java.lang.String"/>
        <property name="numDatabaseSequences" type="java.lang.Long"/>
        <property name="numResidues" type="java.lang.Long"/>
        <property name="databaseName" type="uk.ac.ebi.jmzidml.model.mzidml.ParamAlternative"/>


        <list name="cvParam" cascade="all">
                <key column="hid"/>
                <index column="idx"/>
                <one-to-many class="uk.ac.ebi.jmzidml.model.mzidml.CvParam"/>
        </list>
     </union-subclass>

</hibernate-mapping>
