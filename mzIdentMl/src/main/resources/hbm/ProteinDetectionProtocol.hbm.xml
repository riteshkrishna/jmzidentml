<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <union-subclass name="uk.ac.ebi.jmzidml.model.mzidml.ProteinDetectionProtocol"
                    extends="uk.ac.ebi.jmzidml.model.mzidml.Protocol">

        <many-to-one name="analysisParams" column="ProteinDetectionProtocol_hid" cascade="all" class="uk.ac.ebi.jmzidml.model.mzidml.ParamAlternativeList"/>
        <many-to-one name="threshold" cascade="all" column="ParamAlternativeList_hid" class="uk.ac.ebi.jmzidml.model.mzidml.ParamAlternativeList"/>
        <property name="analysisSoftwareRef" type="java.lang.String"/>

    </union-subclass>
</hibernate-mapping>