<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <union-subclass name="uk.ac.ebi.jmzidml.model.mzidml.AnalysisSearchDatabase" table="AnalysisSearchDatabase"  extends="uk.ac.ebi.jmzidml.model.mzidml.Identifiable">
        <property name="version" type="java.lang.String"/>
        <property name="releaseDate" type="java.lang.String"/>
        <property name="numDatabaseSequences" type="java.lang.Long"/>
        <property name="numResidues" type="java.lang.Long"/>
        <many-to-one name="databaseName" class="uk.ac.ebi.jmzidml.model.mzidml.ParamAlternative"/>

        <list name="cvParam" cascade="all" access="field" table="AnalysisSearchDatabase_CvParam">
                <key column="AnalysisSearchDatabase_hid"/>
                <index column="idx"/>
                <many-to-many column="CvParam_hid" class="uk.ac.ebi.jmzidml.model.mzidml.CvParam"/>
        </list>
     </union-subclass>

</hibernate-mapping>
