<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="uk.ac.ebi.jmzidml.model.mzidml.Affiliations" table="Affiliations" >
        <id name="hid"  type="java.lang.Long" access="field">
            <column name="hid"/>
            <generator class="org.hibernate.id.enhanced.SequenceStyleGenerator"/>
        </id>
        <property name="organizationRef" type="java.lang.String"/>
        <many-to-one name="organization" column="Organization" cascade="all" class="uk.ac.ebi.jmzidml.model.mzidml.Organization"/>
    </class>
</hibernate-mapping>