<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="uk.ac.ebi.jmzidml.model.mzidml.AnalysisProtocolCollection" table="AnalysisProtocolCollection" catalog="mzidentml">
        <id name="hid" type="java.lang.Long">
            <generator class="sequence" />
        </id>

        <list name="spectrumIdentificationProtocol" cascade="all">
            <key column="hid"/>
            <index column="idx"/>
            <one-to-many class="uk.ac.ebi.jmzidml.model.mzidml.SpectrumIdentificationProtocol"/>
        </list>
<!--         <many-to-one name="proteinDetectionProtocol" cascade="all" class="uk.ac.ebi.jmzidml.model.mzidml.ProteinDetectionProtocol"/> -->
        <property name="proteinDetectionProtocol" type="uk.ac.ebi.jmzidml.model.mzidml.ProteinDetectionProtocol"/>
    </class>
</hibernate-mapping>
