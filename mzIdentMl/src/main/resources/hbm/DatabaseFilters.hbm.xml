<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="uk.ac.ebi.jmzidml.model.mzidml.DatabaseFilters" table="DatabaseFilters">
        <id name="hid" access="field" type="java.lang.Long">
            <column name="hid" />
            <generator class="org.hibernate.id.enhanced.SequenceStyleGenerator" />
        </id>
        <list name="filter" cascade="all" access="field" table="DatabaseFilters_Filter">
            <key column="DatabaseFilter_hid"/>
            <index column="idx"/>
            <many-to-many column="Filter_hid" class="uk.ac.ebi.jmzidml.model.mzidml.Filter"/>
    </list>
    </class>
</hibernate-mapping>
