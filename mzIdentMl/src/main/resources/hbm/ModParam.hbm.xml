<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="uk.ac.ebi.jmzidml.model.mzidml.ModParam" table="ModParam" >
        <id name="hid"  type="java.lang.Long" access="field">
            <column name="hid"/>
            <generator class="identity"/>
        </id>
        <property name="massDelta" type="java.lang.Float"/>
        <list name="residues" cascade="all" access="field">
	        <key column="hid" />
	        <list-index column="index" />
	        <element type="java.lang.String"/>
        </list>
    </class>
</hibernate-mapping>