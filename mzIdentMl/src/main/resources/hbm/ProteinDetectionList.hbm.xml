<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<union-subclass name="uk.ac.ebi.jmzidml.model.mzidml.ProteinDetectionList"
		table="ProteinDetectionList" catalog="mzidentml" extends="uk.ac.ebi.jmzidml.model.mzidml.InternalData">

        <list name="cvParam" cascade="all" inverse="true">
            <key column="cvParam_hid" not-null="true"/>
            <index column="cvParam_idx"/>
            <one-to-many class="uk.ac.ebi.jmzidml.model.mzidml.CvParam"/>
        </list>
        
		<list name="userParam" inverse="true" cascade="all">
			<key column="userParam_hid" not-null="true" />
			<index column="userParam_idx" />
			<one-to-many class="uk.ac.ebi.jmzidml.model.mzidml.UserParam" />
		</list>

	</union-subclass>
</hibernate-mapping>
