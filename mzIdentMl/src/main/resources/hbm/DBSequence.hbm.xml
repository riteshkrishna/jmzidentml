<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>


    <union-subclass name="uk.ac.ebi.jmzidml.model.mzidml.DBSequence" table="DBSequence" catalog="mzidentml"
                    extends="uk.ac.ebi.jmzidml.model.mzidml.ConceptualMolecule">
        <property name="seq" type="java.lang.String">

        </property>
        <property name="length" type="java.lang.Integer"/>
        <property name="accession" type="java.lang.String"/>

        <many-to-one name="analysisSearchDatabase" cascade="all"
                     class="uk.ac.ebi.jmzidml.model.mzidml.AnalysisSearchDatabase" access="field"/>

        <list name="paramGroup" cascade="all" access="field">
            <key column="hid"/>
            <index column="idx"/>
            <one-to-many class="uk.ac.ebi.jmzidml.model.mzidml.Param"/>
        </list>
    </union-subclass>
</hibernate-mapping>
