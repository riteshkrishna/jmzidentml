<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <union-subclass name="uk.ac.ebi.jmzidml.model.mzidml.PeptideEvidence" table="PeptideEvidence" catalog="mzidentml" extends="uk.ac.ebi.jmzidml.model.mzidml.Identifiable">
        <list name="paramGroup" cascade="all">
            <key column="paramGroup_hid"/>
            <index column="paramGroup_idx"/>
            <one-to-many class="uk.ac.ebi.jmzidml.model.mzidml.Param"/>
        </list>
        <property name="dbSequenceRef" type="java.lang.String"/>
        <property name="start" type="java.lang.Integer"/>
        <property name="end" type="java.lang.Integer"/>
        <property name="pre" type="java.lang.String"/>
        <property name="post" type="java.lang.String"/>
        <property name="translationTableRef" type="java.lang.String"/>
        <property name="frame" type="java.lang.Integer"/>
        <property name="isDecoy" type="java.lang.Boolean"/>
        <property name="missedCleavages" type="java.lang.Integer"/>
    </union-subclass>
</hibernate-mapping>