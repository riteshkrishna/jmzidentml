<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="uk.ac.ebi.jmzidml.model.mzidml.AnalysisSoftwareList" >
        <id name="hid" type="java.lang.Long">
            <column name="hid" />
            <generator class="org.hibernate.id.enhanced.SequenceStyleGenerator" />
        </id>
        <list name="analysisSoftware" cascade="all" access="field">
            <key column="AnalysisSoftwareList_hid"/>
            <index column="idx"/>
            <many-to-many column="AnalysisSoftware_hid" class="uk.ac.ebi.jmzidml.model.mzidml.AnalysisSoftware"/>
    </list>
    </class>
</hibernate-mapping>
