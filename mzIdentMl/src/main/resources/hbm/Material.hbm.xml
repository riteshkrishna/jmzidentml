<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <union-subclass name="uk.ac.ebi.jmzidml.model.mzidml.Material" abstract="true" catalog="mzidentml"
                    extends="uk.ac.ebi.jmzidml.model.AbstractIdentifiableParamGroup">
        <list name="contactRole" cascade="all" access="field">
            <key column="contactRole_hid"/>
            <index column="contactRole_idx"/>
            <one-to-many class="uk.ac.ebi.jmzidml.model.mzidml.ContactRole"/>
        </list>
        <list name="paramGroup" cascade="all" access="field">
            <key column="paramGroup_hid"/>
            <index column="paramGroup_idx"/>
            <one-to-many class="uk.ac.ebi.jmzidml.model.mzidml.Param"/>
        </list>
    </union-subclass>
</hibernate-mapping>