<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="uk.ac.ebi.jmzidml.model.mzidml.SequenceCollection" table="SequenceCollection" catalog="mzidentml">
        <id name="hid" type="java.lang.Long">
            <column name="hid" />
            <generator class="identity" />
        </id>
        <list name="peptide" inverse="true">
            <key>
                <column name="peptide_hid" not-null="true" />
            </key>
            <index column="peptide_idx"/>
            <one-to-many class="uk.ac.ebi.jmzidml.model.mzidml.Peptide" />
        </list>
        <list name="dbSequence" inverse="true">
            <key>
                <column name="dbSequence_hid" not-null="true" />
            </key>
            <index column="dbSeuence"/>
            <one-to-many class="uk.ac.ebi.jmzidml.model.mzidml.DBSequence" />
        </list>
    </class>
</hibernate-mapping>
