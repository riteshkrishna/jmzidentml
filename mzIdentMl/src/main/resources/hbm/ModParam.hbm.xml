<hibernate-mapping>
    <class name="uk.ac.ebi.jmzidml.model.mzidml.ModParam" table="ModParam" catalog="mzidentml">
        <id name="hid" type="java.lang.Long">
            <column name="hid"/>
            <generator class="sequence"/>
        </id>
        <property name="massDelta" type="java.lang.Float"/>
        <list name="residues">
	        <key column="hid" />
	        <list-index column="index" />
	        <element type="java.lang.String"/>
        </list>
    </class>
</hibernate-mapping>