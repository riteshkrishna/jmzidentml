<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="uk.ac.ebi.jmzidml.model.mzidml.SpectrumIDFormat"
		table="SpectrumIDFormat" catalog="mzidentml">
		<id name="hid" type="java.lang.Long">
			<column name="hid" />
			<generator class="identity" />
		</id>
		<many-to-one name="cvParam" cascade="all" column="cvParam_hid"
			not-null="true" class="uk.ac.ebi.jmzidml.model.mzidml.CvParam"/>
	</class>
</hibernate-mapping>
