<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

    <union-subclass name="uk.ac.ebi.jmzidml.model.mzidml.AmbiguousResidue" table="AmbigousResidue" catalog="mzidentml" extends="uk.ac.ebi.jmzidml.model.AbstractParamGroup">
        <list name="paramGroup" cascade="all">
